  "dependencies": {
    "required": {
      "php": {
        "min": "5.6.0",
        "max": "7.2.0"
      }
    }
  },
  "include": {
    "magelocal": [] ,
    "magecommunity": [
      "app/code/community/Emartech/Emarsys/*"
