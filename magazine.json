{
  "name": "Emarsys",
  "version": {
    "release": "1.2.0"
  },
  "stability": {
    "release": "stable"
  },
  "license": {
    "_content": "MIT",
    "attribs": {
      "uri": "https://github.com/emartech/magento1-extension/blob/master/LICENSE"
    }
  },
  "channel": "community",
  "summary": "Emarsys Magento 1 Extension",
  "description": "Connector for Emarsys Marketing Platform",
  "developer": {
    "name": "Emarsys",
    "user": "team-plugins",
    "email": "team-shopify@emarsys.net"
  },
